{
    "compile_me": "opencv-foo.cpp",
    "include_regex": "opencv",
    "output_dir": "../generated-opencv/",
    "//comment": "xsfile is for old version, so we can run both and compare",
    "xsfile": "OpenCV.xs",
    "xs_filename": "OpenCV.xs",
    "base_module": "OpenCV",
    "includes": "#undef NORMAL\n#include <opencv2/opencv.hpp>",
    "ignore_all_macros": 1,
    "ignore_macros": ["__OPENCV_ALL_HPP__",
                      "__OPENCV_CORE_C_H__",
                      "__OPENCV_CORE_TYPES_H__",
                      "CV_CDECL",
                      "CV_STDCALL",
                      "CV_EXTERN_C",
                      "CV_DEFAULT",
                      "CV_EXTERN_C_FUNCPTR",
                      "CV_DEFAULT",
                      "CV_EXPORTS",
                      "CV_INLINE",
                      "CVAPI",
                      "CV_BIG_INT",
                      "CV_ARRAY",
                      "CV_CUSTOM_CARRAY",
                      "CV_BIG_UINT",
                     ],
    "opencv_hack": 1,
}